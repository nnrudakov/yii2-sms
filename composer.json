{
  "name": "nnrudakov/yii2-sms",
  "description": "Send SMS via different services in Yii2.",
  "keywords": [
    "yii2",
    "sms",
    "gateway",
    "beeline"
  ],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Nikolaj Rudakov",
      "email": "nnrudakov@gmail.com"
    }
  ],
  "support": {
    "email": "nnrudakov@gmail.com",
    "issues": "https://github.com/nnrudakov/yii2-sms/issues",
    "source": "https://github.com/nnrudakov/yii2-sms"
  },
  "type": "library",
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": ">=8.1",
    "yiisoft/yii2": "~2.0.41",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "codeception/codeception": "^4.0",
    "codeception/verify": "^1.0",
    "codeception/specify": "^1.0",
    "codeception/module-filesystem": "^1.0",
    "codeception/module-yii2": "^1.0",
    "codeception/module-asserts": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "nnrudakov\\": "src/"
    }
  }
}
